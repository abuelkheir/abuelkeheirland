#include "STD_TYPES.h"
#include"Atmega32Registers.h"
#include"BIT_MATH.h"
#include"LCD_int.h"
#include"DIO_int.h"
#include"Keypad_int.h"

int main(void)
{
	LCD_vidInit();
	Keypad_vidInit();
	u8 KeyVal;
	while(1)
	{
		KeyVal = Keypad_u8GetKey();
		if(KeyVal != NOT_PRESSED)
			LCD_vidWriteData(KeyVal);
	}
	return 0;
}
